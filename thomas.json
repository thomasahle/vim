            "key_code": "quote",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "o",
              "modifiers": [
                "left_option"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": [
                "option",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "o",
              "modifiers": [
                "left_option",
                "left_shift"
              ]
            }
