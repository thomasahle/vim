            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote",
              "modifiers": [
                "left_option"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "option",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote",
              "modifiers": [
                "left_option",
                "left_shift"
              ]
            }
          ]
        },

        {
          "type": "basic",
